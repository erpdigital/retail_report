k;\">{{ _(\"Mukdary\") }}</th>\r\n\r\n\t\t\t<th width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ _(\"Baha\") }}</th>\r\n\t\t\t<th width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ _(\"Jemi\") }}</th>\r\n\t\t</tr>\r\n\t</thead>\r\n\t<tbody>\r\n\t\t{%- for item in doc.items -%}\r\n\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td width=\"40%\" class=\"text-left\" style=\"border: 1px solid black;\">{{ item.item_name }}\t</td>\r\n\t\t\t<td width=\"5%\" class=\"text-left\" style=\"border: 1px solid black;\">{% if item.qty%1 != 0 %}\r\n                  {{ item.qty|float }}\r\n                  {% else %}\r\n                  {{ item.qty|int }}\r\n                  {% endif %} {{ item.uom }} </td>\r\n\t\t\t<td width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\"> {% if item.rate%1 != 0 %}\r\n                  {{ item.rate|float }}\r\n                  {% else %}\r\n                  {{ item.rate|int }}\r\n                  {% endif %}</td>\r\n\t\t\t<td width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\">{% if item.amount%1 != 0 %}\r\n                  {{ item.amount|float }}\r\n                  {% else %}\r\n                  {{ item.amount|int }}\r\n                  {% endif %} </td>\r\n\t\t</tr>\r\n\t\t{%- endfor -%}\r\n\t</tbody>\r\n</table>\r\n<table class=\"table table-condensed no-border\" style=\"border-collapse: collapse; width: 100%;\">\r\n\t<tbody>\r\n\t       \r\n\t\t{%- if doc.discount_amount -%}\r\n\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n\t\t\t\t{{ _(\"Discount\") }}\r\n\t\t\t</td>\r\n\t\t\t<td class=\"text-left\" style=\"width: 60%; border: 1px solid black;\">\r\n\t\t\t\t{{ doc.discount_amount }}\r\n\t\t\t</td>\r\n\t\t</tr>\r\n\t\t{%- endif -%}\r\n\t\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n\t\t\t\t<b>{{ _(\"Jemi Mukdar:\") }}</b>\r\n\t\t\t</td>\r\n\t\t\t<td class=\"text-left\" style=\"width: 60%; border: 1px solid black;\">\r\n\t\t\t\t{{ doc.total_qty }}\r\n\t\t\t</td>\r\n\t\t</tr>\r\n\t\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n\t\t\t\t<b>{{ _(\"Hasap\") }}</b>\r\n\t\t\t</td>\r\n\t\t\t<td class=\"text-left\" style=\"width: 60%; border: 1px solid black;\">\r\n\t\t\t\t{{ doc.grand_total }}\r\n\t\t\t</td>\r\n\t\t</tr>\r\n\t\t  {% if doc.previous_credit %}\r\n                  {% set credit = doc.previous_credit|float %}\r\n              <tr style=\"border: 1px solid black;\">\r\n                  \t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n                  \t\t    {{ _(\"Karzy:\") }}</td>\r\n                  <td class=\"text-left\">  {{ '%0.2f'| format(credit|float) }}  \r\n                  </td>\r\n               </tr>\r\n               {% endif %}\r\n\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n\t\t\t\t<b>{{ _(\"Umumy Hasap\") }}</b>\r\n\t\t\t</td>\r\n\t\t\t{% if doc.previous_credit %}\r\n                  <td class=\"text-left\"><strong>\r\n                     {% set sum =doc.grand_total +doc.previous_credit|float %}\r\n                     {% if sum%1 != 0 %}\r\n                     {{ '%0.2f'| format(sum|float) }}\r\n                     {% else %}\r\n                     {{ sum|int }}\r\n                     {% endif %}\r\n                     M</strong>\r\n                     {% else %}\r\n                  <td class=\"text-left\"><strong> {{ '%0.2f'| format(doc.grand_total|float) }} </strong>\r\n                     {% endif %}\r\n                  </td>\r\n\t\t</tr>\r\n\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 30%\">\r\n\t\t\t\t<b>{{ _(\"Tolenen Hasap\") }}</b>\r\n\t\t\t</td>\r\n\t\t\t<td class=\"tex