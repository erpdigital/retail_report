 "html": "<style>\r\n\t.print-format table,\r\n\t.print-format tr,\r\n\t.print-format td,\r\n\t.print-format div,\r\n\t.print-format p {\r\n\t\tline-height: 150%;\r\n\t\tvertical-align: middle;\r\n\t}\r\n\r\n\t@media screen {\r\n\t\t.print-format {\r\n\t\t\t\twidth: 4in;\r\n\t\t\tpadding: 0.25in;\r\n\t\t    margin:3px;\r\n\t\t\tmin-height: 8in;\r\n\t\t}\r\n\t}\r\n\r\n\ttable {\r\n\t\twidth: 100%;\r\n\t\tborder-collapse: collapse;\r\n\t}\r\n\r\n\ttd,\r\n\tth {\r\n\t\tborder: 1px solid #000;\r\n\t\tpadding: 5px;\r\n\t\ttext-align: right;\r\n\t}\r\n\r\n\tth {\r\n\t\ttext-align: center;\r\n\t}\r\n\r\n\t.text-center {\r\n\t\ttext-align: center;\r\n\t}\r\n\r\n\t.text-left {\r\n\t\ttext-align: left;\r\n\t}\r\n\r\n\t.no-border td,\r\n\t.no-border th {\r\n\t\tborder: none;\r\n\t}\r\n\r\n\t.bold {\r\n\t\tfont-weight: bold;\r\n\t}\r\n\r\n\t.uppercase {\r\n\t\ttext-transform: uppercase;\r\n\t}\r\n\r\n\t.line-through {\r\n\t\ttext-decoration: line-through;\r\n\t}\r\n</style>\r\n\r\n{% if letter_head %}\r\n    {{ letter_head }}\r\n{% endif %}\r\n\r\n<p class=\"text-center\" style=\"margin-bottom: 1rem\">\r\n\t{{ doc.company }}<br>\r\n\t<b class=\"uppercase\">{{ doc.select_print_heading or _(\"Invoice\") }}</b><br>\r\n</p>\r\n<p>\r\n\t<b>{{ _(\"Receipt No\") }}:</b> {{ doc.name }}<br>\r\n\t<b>{{ _(\"Cashier\") }}:</b> {{ doc.owner }}<br>\r\n\t<b>{{ _(\"Customer\") }}:</b> {{ doc.customer_name }}<br>\r\n\t<b>{{ _(\"Date\") }}:</b> {{ doc.get_formatted(\"posting_date\") }}<br>\r\n\t<b>{{ _(\"Time\") }}:</b> {{  doc.get_formatted(\"posting_time\") }}<br>\r\n</p>\r\n\r\n<hr>\r\n\t\t\t<table class=\"table table-condensed\" style=\"border-collapse: collapse; width: 100%;\">\r\n\t<thead>\r\n\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<th width=\"40%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ _(\"Haryt Ady\") }}</th>\r\n\t\t\t<th width=\"5%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ _(\"Mukdary\") }}</th>\r\n\r\n\t\t\t<th width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ _(\"Baha\") }}</th>\r\n\t\t\t<th width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ _(\"Jemi\") }}</th>\r\n\t\t</tr>\r\n\t</thead>\r\n\t<tbody>\r\n\t\t{%- for item in doc.items -%}\r\n\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td width=\"40%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ item.item_name }}\t</td>\r\n\t\t\t<td width=\"5%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ item.qty }} {{ item.stock_uom }} </td>\r\n\t\t\t<td width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\"> {{ item.rate }}</td>\r\n\t\t\t<td width=\"10%\" class=\"text-right\" style=\"border: 1px solid black;\">{{ item.amount }}</td>\r\n\t\t</tr>\r\n\t\t{%- endfor -%}\r\n\t</tbody>\r\n</table>\r\n<table class=\"table table-condensed no-border\" style=\"border-collapse: collapse; width: 100%;\">\r\n\t<tbody>\r\n\t\t{%- if doc.discount_amount -%}\r\n\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n\t\t\t\t{{ _(\"Discount\") }}\r\n\t\t\t</td>\r\n\t\t\t<td class=\"text-left\" style=\"width: 60%; border: 1px solid black;\">\r\n\t\t\t\t{{ doc.discount_amount }}\r\n\t\t\t</td>\r\n\t\t</tr>\r\n\t\t{%- endif -%}\r\n\t\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n\t\t\t\t<b>{{ _(\"Jemi Mukdar:\") }}</b>\r\n\t\t\t</td>\r\n\t\t\t<td class=\"text-left\" style=\"width: 60%; border: 1px solid black;\">\r\n\t\t\t\t{{ doc.total_qty }}\r\n\t\t\t</td>\r\n\t\t</tr>\r\n\t\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 40%; border: 1px solid black;\">\r\n\t\t\t\t<b>{{ _(\"Umumy Hasap\") }}</b>\r\n\t\t\t</td>\r\n\t\t\t<td class=\"text-left\" style=\"width: 60%; border: 1px solid black;\">\r\n\t\t\t\t{{ doc.grand_total }}\r\n\t\t\t</td>\r\n\t\t</tr>\r\n\t<tr style=\"border: 1px solid black;\">\r\n\t\t\t<td class=\"text-left\" style=\"width: 30%\">\r\n\t\t\t\t<b>{{ _(\"Tolenen Hasap\") }}</b>\r\n\t\t\t</td>\r\n\t\t\t<td class=\"text-left\">\r\n\t\t\t\t{{ doc.paid_amount }}\r\n\t\t\t</td>\r\n\t\t</tr>\r\n\t\t{%- if doc.change_amount -%}\r\n\t\t\t<tr>\r\n\t\t\t\t<td class=\"text-left\" style=\"width: 30%\">\r\n\t\t\t\t\t<b>{{ _(\"Change Amount\") }}</b>\r\n\t\t\t\t</td>\r\n\t\t\t\t<td class=\"text-left\">\r\n\t\t\t\t\t{{ doc.change_amount }}\r\n\t\t\t\t</td>\r\n\t\t\t</tr>\r\n\t\t{%- endif -%}\r\n\t</tbody>\r\n</table>\r\n<hr>\r\n\r\n<p class=\"text-center\">{{ _(\"Thank you, please visit again.\") }}</p>",
